  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "NODE_ENV=production ng build",
    "build:prod": "NODE_ENV=production ng build --configuration production --output-hashing none",
    "dev": "npm run build && npm run scully && npm run serve:scully",
    "build:scully": "npm run build:prod && npm run scully",
    "build:elements": "NODE_ENV=production ng build --configuration production --project elements --single-bundle --output-hashing none",
    "build:demos": "ng build --configuration production --project demos",
    "format": "prettier --write \"src/**/*.{ts,scss}\"",
    "scully": "scully",
    "serve:scully": "scully serve",
    "scully:ci": "npm run scully -- --host='0.0.0.0' --scanRoutes --serverTimeout=60000",
    "optimize:images": "node optimize-images.js",
    "codegen:api": "npx ng-openapi-gen -i https://notiz-dev-api.herokuapp.com/api-json -o src/api"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "12.2.7",
    "@angular/cdk": "^12.2.7",
    "@angular/common": "12.2.7",
    "@angular/compiler": "12.2.7",
    "@angular/core": "12.2.7",
    "@angular/elements": "12.2.7",
    "@angular/forms": "12.2.7",
    "@angular/platform-browser": "12.2.7",
    "@angular/platform-browser-dynamic": "12.2.7",
    "@angular/router": "12.2.7",
    "@angular/service-worker": "12.2.7",
    "@ngneat/hot-toast": "3.4.1",
    "@ngneat/overview": "2.0.4",
    "@notiz/shortcodes": "0.2.0",
    "@notiz/toolbelt": "0.0.3",
    "@scullyio/ng-lib": "2.0.0",
    "@scullyio/scully": "2.0.0",
    "document-register-element": "1.14.10",
    "ngx-markdown": "12.0.1",
    "ngx-plausible": "0.0.5",
    "prismjs": "1.24.1",
    "rough-notation": "^0.5.1",
    "rxjs": "7.3.0",
    "tslib": "2.3.1",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "12.2.7",
